{
    "version": 2,
    "builds": [
        {
            "src": "frontend/build/**",
            "use": "@vercel/static"
        },
        {
            "src": "backend/app.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/backend/(.*)",
            "dest": "backend/app.js"
        },
        {
            "src": "/(.*)",
            "dest": "frontend/build/index.html"
        }
    ]
}